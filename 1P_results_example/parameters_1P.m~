%% primary values

% measurement parameters
s1 = 0.02; % microphone separation 1
s2 = 0.02; % microphone separation 2

L = 0.1; % distance mic-object side A
D = 0.29; % tube diameter

% physical constants
rho0 = 1.18; % air density
c0 = 346; % speed of sound
pref = 2e-5; % reference pressure

% frequency parameters
fmax=6400;
fmin=100;

% any comments, to describe the test
comments = '4cm thick grey foam';


%% derived values

x1a = L;
x2a = L - s1;
x3a = L - s1 - s2;

Z0 = rho0*c0; % plane wave impedance
S = pi*(D/2)^2; % tube section

